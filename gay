if game.CoreGui:FindFirstChild('UIBUTTON') then game.CoreGui:FindFirstChild('UIBUTTON'):Destroy() end;local TweenService=game:GetService("TweenService");local UserInputService=game:GetService("UserInputService");local UIBUTTON=Instance.new("ScreenGui");local Frame=Instance.new("Frame");local ImageButton=Instance.new("ImageButton");local UICorner=Instance.new("UICorner");local UIStroke=Instance.new("UIStroke");UIBUTTON.Name="UIBUTTON";UIBUTTON.Parent=game.CoreGui;UIBUTTON.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;UIBUTTON.ResetOnSpawn=false;Frame.Parent=UIBUTTON;Frame.BackgroundTransparency=1;Frame.Position=UDim2.new(0.15,0,0.16,0);Frame.Size=UDim2.new(0,50,0,50);ImageButton.Parent=Frame;ImageButton.BackgroundColor3=Color3.fromRGB(30,30,30);ImageButton.BorderSizePixel=0;ImageButton.Position=UDim2.new(0,0,0,0);ImageButton.Size=UDim2.new(1,0,1,0);ImageButton.Image="http://www.roblox.com/asset/?id=77704093714738";ImageButton.ScaleType=Enum.ScaleType.Fit;ImageButton.ImageColor3=Color3.fromRGB(245,151,221);ImageButton.SliceCenter=Rect.new(0,0,0,0);ImageButton.SliceScale=1;ImageButton.AnchorPoint=Vector2.new(0.5,0.5);ImageButton.Position=UDim2.new(0.5,0,0.5,0);local UIPadding=Instance.new("UIPadding");UIPadding.Parent=ImageButton;UIPadding.PaddingTop=UDim.new(0.2,0);UIPadding.PaddingBottom=UDim.new(0.2,0);UIPadding.PaddingLeft=UDim.new(0.2,0);UIPadding.PaddingRight=UDim.new(0.2,0);UIStroke.Parent=ImageButton;UIStroke.Thickness=3;UIStroke.Color=Color3.fromRGB(255,150,150);UIStroke.ApplyStrokeMode=Enum.ApplyStrokeMode.Border;UICorner.CornerRadius=UDim.new(1,0);UICorner.Parent=ImageButton;local dragging=false;local dragInput;local dragStart;local startPos;local function update(input)local delta=input.Position-dragStart;Frame.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset+delta.X,startPos.Y.Scale,startPos.Y.Offset+delta.Y)end;ImageButton.InputBegan:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then if not dragging then dragging=true;dragStart=input.Position;startPos=Frame.Position;input.Changed:Connect(function()if input.UserInputState==Enum.UserInputState.End then dragging=false end end)end end end);ImageButton.InputChanged:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch then dragInput=input end end);UserInputService.InputChanged:Connect(function(input)if input==dragInput and dragging then update(input)end end);ImageButton.MouseButton1Click:Connect(function()local clickEffect=TweenService:Create(ImageButton,TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size=UDim2.new(0.9,0,0.9,0),Position=UDim2.new(0.5,0,0.5,0)});clickEffect:Play();clickEffect.Completed:Connect(function()local returnEffect=TweenService:Create(ImageButton,TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size=UDim2.new(1,0,1,0),Position=UDim2.new(0.5,0,0.5,0)});returnEffect:Play()end);game:GetService("VirtualInputManager"):SendKeyEvent(true,Enum.KeyCode.LeftControl,false,game);print("Button clicked - End key sent")end);spawn(function()while wait(1)do local pulse=TweenService:Create(UIStroke,TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{Transparency=0.5});pulse:Play();pulse.Completed:Wait();local pulse2=TweenService:Create(UIStroke,TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{Transparency=0});pulse2:Play();pulse2.Completed:Wait()end end)
